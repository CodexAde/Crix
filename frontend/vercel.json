{
  "headers": [
    {
      "source": "/(index.html|sw.js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    { 
      "source": "/api/:path*", 
      "destination": "https://crix-or5y.onrender.com/api/:path*"
    },
    { 
      "source": "/((?!assets/|favicon.png|manifest.json|sw.js).*)", 
      "destination": "/index.html" 
    }
  ]
}